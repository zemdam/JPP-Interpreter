int x = 2;

if (x >= 1) print "x >= 1";

if (x > 2) {
    print "x > 2";
} else {
    print "x <= 2";
} // Should print "x <= 2";

while (x < 5) {
    print x;
    x++;
} // Should print "2" "3" "4"

while (x > 0) {
    if (x > 2) {
        print "x > 2";
    } else {
        print "x <= 2";
    }
    x--;
} // Should print "x > 2" "x > 2" "x > 2" "x <= 2" "x <= 2"