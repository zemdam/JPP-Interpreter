# Makefile for building the parser and test program.

GHC        = ghc
HAPPY      = happy
HAPPY_OPTS = --array --info --ghc --coerce
ALEX       = alex
ALEX_OPTS  = --ghc

# List of goals not corresponding to file names.

.PHONY : all clean distclean

# Default goal.

all : Bnfc/Test

# Rules for building the parser.

Bnfc/Abs.hs Bnfc/Lex.x Bnfc/Par.y Bnfc/Print.hs Bnfc/Test.hs : bnfc.cf
	bnfc --haskell -d --functor bnfc.cf

%.hs : %.y
	${HAPPY} ${HAPPY_OPTS} $<

%.hs : %.x
	${ALEX} ${ALEX_OPTS} $<

Bnfc/Test : Bnfc/Abs.hs Bnfc/Lex.hs Bnfc/Par.hs Bnfc/Print.hs Bnfc/Test.hs
	${GHC} ${GHC_OPTS} $@

# Rules for cleaning generated files.

clean :
	-rm -f Bnfc/*.hi Bnfc/*.o Bnfc/*.log Bnfc/*.aux Bnfc/*.dvi

distclean : clean
	-rm -f Bnfc/Abs.hs Bnfc/Abs.hs.bak Bnfc/ComposOp.hs Bnfc/ComposOp.hs.bak Bnfc/Doc.txt Bnfc/Doc.txt.bak Bnfc/ErrM.hs Bnfc/ErrM.hs.bak Bnfc/Layout.hs Bnfc/Layout.hs.bak Bnfc/Lex.x Bnfc/Lex.x.bak Bnfc/Par.y Bnfc/Par.y.bak Bnfc/Print.hs Bnfc/Print.hs.bak Bnfc/Skel.hs Bnfc/Skel.hs.bak Bnfc/Test.hs Bnfc/Test.hs.bak Bnfc/XML.hs Bnfc/XML.hs.bak Bnfc/AST.agda Bnfc/AST.agda.bak Bnfc/Parser.agda Bnfc/Parser.agda.bak Bnfc/IOLib.agda Bnfc/IOLib.agda.bak Bnfc/Main.agda Bnfc/Main.agda.bak Bnfc/bnfc.dtd Bnfc/bnfc.dtd.bak Bnfc/Test Bnfc/Lex.hs Bnfc/Par.hs Bnfc/Par.info Bnfc/ParData.hs Makefile
	-rmdir -p Bnfc/

